doctype xml
rss(version="2.0")
	channel
		title JoshAlerts
		link http://joshalerts.azurewebsites.net
		description Just an RSS feed for my alerts
		each alert in alerts
			item
				title= alert.title + ' | ' + alert.priority 
				link http://joshalerts.azurewebsites.net
				description= alert.description
				pubDate= alert.created